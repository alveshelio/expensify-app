{
  "name": "Expensify-App",
  "version": "0.0.1",
  "private": false,
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "client:dev": "webpack-dashboard -t 'Marvin' -- webpack-dev-server",
    "client:watch": "webpack",
    "client:build": "npm run clean && cross-env NODE_ENV=\"production\" webpack",
    "client:preview": "npm run clean && cross-env NODE_ENV=\"production\" webpack-dev-server",
    "server:watch": "cross-env SERVER_RENDER=\"true\" webpack --config webpack.config.server.js",
    "server:restart": "nodemon ./build/server.js",
    "server:build": "cross-env SERVER_RENDER=\"true\" cross-env NODE_ENV=\"production\" webpack --config webpack.config.server.js",
    "server:dev": "npm run clean && concurrently --prefix \"[{name}]\" --names \"webpack-server,watch-server\" --kill-others \"npm run server:watch\" \"npm run server:restart\"",
    "universal:dev": "concurrently --prefix \"[{name}]\" --names \"webpack-server,webpack-client,watch-server\" --kill-others \"npm run server:watch\" \"npm run client:watch\" \"npm run server:restart\"",
    "universal:build": "npm run client:build && npm run server:build",
    "clean": "rm -rf ./build",
    "lint-hook": "eslint ./source/js ./webpack ./*.js -f table --ext .js --ext .jsx",
    "lint": "npm run lint-hook --silent || true",
    "hook-add": "prepush install",
    "hook-remove": "prepush remove",
    "test": "jest"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "concurrently": "^3.5.0",
    "cross-env": "^5.1.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "postcss-loader": "^2.0.8",
    "prepush": "^3.1.11",
    "react-hot-loader": "^3.1.1",
    "react-svg-loader": "^2.1.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "svgo": "^0.7.2",
    "svgo-loader": "^1.2.1",
    "webpack": "^3.8.1",
    "webpack-dashboard": "^1.0.0",
    "webpack-dev-server": "^2.9.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "es6-promise": "^4.1.1",
    "eslint": "^4.9.0",
    "express": "^4.16.2",
    "file-loader": "^1.1.5",
    "immutable": "^3.8.2",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.19.1",
    "moment-range": "^3.0.3",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-addons-shallow-compare": "^15.6.2",
    "react-dates": "^12.7.0",
    "react-dom": "^16.0.0",
    "react-infinite-calendar": "^2.3.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-with-styles": "^2.2.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "transit-immutable-js": "^0.7.0",
    "transit-js": "^0.8.846",
    "twix": "^1.2.1",
    "uuid": "^3.1.0"
  },
  "description": "Starter boilerplate for React and Redux, using Webpack 3",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/workco/marvin.git"
  },
  "author": "Stanko",
  "prepush": [
    "npm run lint-hook --silent"
  ]
}
